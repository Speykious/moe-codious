	"tokenColors": [
		{
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#fcffe4",
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.semi",
				"punctuation.terminator",
				"punctuation.eq.toml",
				"punctuation.squarebracket",
				"markup.quote.markdown",
			],
			"settings": {
				"foreground": "#fcffe480"
			}
		},
		{
			"scope": [
				"punctuation.comma",
				"punctuation.separator.comma",
				"punctuation.definition.table.inline.toml",
				"punctuation.definition.tag",
				"punctuation.accessor",
				"punctuation.separator.dot.toml",
				"punctuation.separator.colon.python",
				"punctuation.separator.arguments.python",
				"punctuation.separator.key-value",
			],
			"settings": {
				"foreground": "#fcffe4c0"
			}
		},
		{
			"scope": [
				"meta.table.inline.toml",
			],
			"settings": {
				"foreground": "#fcffe4d0"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"keyword.operator.expression.new",
				"markup.underline.link.markdown",
			],
			"settings": {
				"foreground": "#ff79b5"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"punctuation.separator.pointer-access",
				"keyword.operator.comparison.perl",
			],
			"settings": {
				"foreground": "#ffffaaaa",
				"fontStyle": "",
			}
		},
		{
			"scope": [
				"keyword.operator.math",
				"keyword.operator.arithmetic",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.increment",
				"keyword.operator.conditional",
				"keyword.operator.ternary",
			],
			"settings": {
				"foreground": "#ffaa20",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"constant",
				"variable.other.constant",
				"variable.other.object.property",
				"variable.other.env",
				"support.variable.property",
				"meta.attribute.python",
			],
			"settings": {
				"foreground": "#ff98b8",
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language.bool",
				"constant.language.boolean",
				"constant.language.java",
				"constant.language.json",
				"entity.other.attribute-name.id",
			],
			"settings": {
				"foreground": "#ffa358",
			}
		},
		{
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#ffc466",
			}
		},
		{
			"scope": [
				"entity.name.type",
				"support.type.python",
				"support.type.primitive.ts",
			],
			"settings": {
				"foreground": "#41ffd5",
			}
		},
		{
			"scope": [
				"entity.name.section.markdown",
				"punctuation.definition.heading.markdown",
			],
			"settings": {
				"foreground": "#41ffd5",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"storage.type.java",
				"entity.other.inherited-class",
			],
			"settings": {
				"foreground": "#ffa34d",
			}
		},
		{
			"scope": [
				"entity.name.type.numeric",
				"storage.type.primitive",
				"support.type.primitive",
				"markup.italic.markdown",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"entity.other.attribute-name.class",
			],
			"settings": {
				"foreground": "#ffffaa",
			}
		},
		{
			"scope": [
				"variable.other.object",
			],
			"settings": {
				"foreground": "#ffffca",
			}
		},
		{
			"scope": [
				"punctuation.definition.annotation",
				"storage.type.annotation",
			],
			"settings": {
				"foreground": "#ffffaa",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"entity.name.function",
				"meta.function-call.generic",
				"support.function",
			],
			"settings": {
				"foreground": "#6acbff"
			}
		},
		{
			"scope": [
				"variable.language.self",
				"markup.bold.markdown",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"storage.modifier.mut.rust",
				"support.type.property-name.table.toml",
				"keyword.operator.expression.typeof",
				"keyword.operator.expression.nameof",
			],
			"settings": {
				"foreground": "#ff6355",
			}
		},
		{
			"scope": [
				"meta.interpolation",
			],
			"settings": {
				"foreground": "#feaa9a",
			}
		},
		{
			"scope": [
				"entity.name.tag.yaml",
				"variable.css",
				"variable.other.readwrite.global.perl",
				"variable.other.assignment.shell",
				"variable.other.normal.shell",
				"support.type.property-name",
				"support.type.primitive",
				"entity.other.attribute-name",
				"punctuation.definition.interpolation",
				"punctuation.definition.lifetime",
				"punctuation.definition.variable.perl",
				"punctuation.definition.variable.shell",
				"entity.name.type.lifetime",
			],
			"settings": {
				"foreground": "#ff877c",
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"support.class.component",
				"meta.selector.css",
			],
			"settings": {
				"foreground": "#ff3978",
			}
		},
		{
			"scope": [
				"keyword.operator.borrow.and",
				"keyword.operator.dereference",
				"variable.other.predefined.perl",
			],
			"settings": {
				"foreground": "#ff877c",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"meta.preprocessor.macro.c",
				"entity.name.function.preprocessor.c",
				"entity.name.function.macro",
				"keyword.operator.question.rust",
			],
			"settings": {
				"foreground": "#ff6355",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"constant.character.escape",
			],
			"settings": {
				"foreground": "#ffa29a",
			}
		},
		{
			"scope": [
				"meta.attribute.rust",
				"entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element",
			],
			"settings": {
				"foreground": "#ffa29aa0",
				"fontStyle": "italic",
			}
		},
	]
}